kind: RoleBinding
apiVersion: rbac.authorization.k8s.io/v1beta1
metadata:
  name: dojot-nginx-ingress
  namespace: {{ dojot_namespace }}
subjects:
- kind: ServiceAccount
  name: nginx-ingress
  namespace: {{ dojot_ingress_namespace }}
roleRef:
  kind: Role
  name: dojot-nginx-ingress
  apiGroup: rbac.authorization.k8s.io